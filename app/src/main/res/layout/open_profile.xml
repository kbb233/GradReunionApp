<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/greenbg"
    tools:context=".OpenProfile">


    <TextView
        android:id="@+id/aboutMe_text2"
        android:layout_width="241dp"
        android:layout_height="27dp"
        android:layout_marginStart="86dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="84dp"
        android:text="What's up with current me:"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gradYear_text2" />

    <TextView
        android:id="@+id/gradYear_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="17dp"
        android:text="Grad Year:"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name_text3" />

    <TextView
        android:id="@+id/name_text3"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_marginStart="51dp"
        android:layout_marginTop="264dp"
        android:text="Name:"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/profile_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:layout_marginStart="297dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="297dp"
        android:text="Profile"
        android:textAlignment="textStart"
        android:textSize="35sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_name"
        android:layout_width="249dp"
        android:layout_height="27dp"
        android:layout_marginTop="204dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="sans-serif-black"
        android:text="TextView"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_text"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/textView_gradYear"
        android:layout_width="254dp"
        android:layout_height="26dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="14dp"
        android:fontFamily="sans-serif-black"
        android:text="TextView"
        android:textSize="20sp"
        android:textStyle="bold"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_name"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/textView_currentMe"
        android:layout_width="350dp"
        android:layout_height="158dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="30dp"
        android:fontFamily="sans-serif-thin"
        android:text="TextView"
        android:textSize="20sp"
        android:textStyle="bold"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/aboutMe_text2"
        tools:visibility="invisible" />


</androidx.constraintlayout.widget.ConstraintLayout>